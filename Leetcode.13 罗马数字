class Solution:
    def romanToInt(self,s:str) -> int:
        roman_map_special={'CD':400,'CM':900,'XL':40,'XC':90,'IV':4,'IX':9}
        roman_map_normal = {'I':1,'V':5,'X':10,'L':50,'C':100,'D':500,'M':1000}
        output = 0
        i=0
        while i< len(s):
            if i+1<len(s) and s[i:i+2] in roman_map_special:
                output += roman_map_special[s[i:i+2]]
                i+=2
            else:
                output += roman_map_normal[s[i]]
                i+=1
        return output